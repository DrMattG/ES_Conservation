@article{ ISI:000427546800013,
Author = {Koch, Heinz-Josef},
Title = {{Of load and pressure: what can our soils bear?}},
Journal = {{SUGAR INDUSTRY-ZUCKERINDUSTRIE}},
Year = {{2018}},
Volume = {{143}},
Number = {{2}},
Pages = {{88-93}},
Month = {{FEB}},
Abstract = {{A favorable soil structure is essential for an efficient production of
   crops. In this context, special focus must be directed towards the
   protection of the subsoil against structural degradation because
   regeneration is almost impossible. Studies conducted in the years
   2000-2005 evaluated the soil structure of arable fields as satisfying to
   good, however, more recent investigations are lacking. A meta -analysis
   of field trials performed with fully loaded 6 -row bunker harvesters
   widely used in German sugar beet cropping clearly indicated a
   degradation of functional soil structural properties under wet soil
   conditions, although data showed large variation. Nevertheless, only few
   studies revealed a substantial yield decrease in subsequent field crops.
   During the harvest period (September to November), in most German sugar
   beet cropping regions high subsoil moisture close to field capacity is
   prevalent in few years only. Thus, in most years the risk of increasing
   subsoil compaction is low. Recent improvements of tires and chassis were
   beneficial to the topsoil but not the subsoil structure.}},
