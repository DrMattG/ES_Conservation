@article{ ISI:000460664300012,
Author = {Stanford, Bronwen and Jones, Holly and Zavaleta, Erika},
Title = {{Meta-analysis of the effects of upstream land cover on stream recovery}},
Journal = {{CONSERVATION BIOLOGY}},
