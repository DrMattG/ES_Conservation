@article{ ISI:000380746000002,
Author = {He, Qiang and Silliman, Brian R.},
Title = {{Consumer control as a common driver of coastal vegetation worldwide}},
Journal = {{ECOLOGICAL MONOGRAPHS}},
Year = {{2016}},
Volume = {{86}},
