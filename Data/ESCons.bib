@article{ ISI:000401928400007,
Author = {Zhang, Luoyan and Zhang, Xuejie and Fan, Shoujin},
Title = {{Meta-analysis of salt-related gene expression profiles identifies common
   signatures of salt stress responses in Arabidopsis}},
