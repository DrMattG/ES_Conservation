@article{ ISI:000511619900003,
Author = {Feng, Xikang and Wang, Zishuai and Li, Hechen and Li, Shuai Cheng},
Title = {{MIRIA: a webserver for statistical, visual and meta-analysis of RNA
   editing data in mammals}},
