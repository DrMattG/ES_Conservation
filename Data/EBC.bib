@article{ ISI:000278650600011,
Author = {Gusset, Markus and Stewart, Gavin B. and Bowler, Diana E. and Pullin,
   Andrew S.},
Title = {{Wild dog reintroductions in South Africa: A systematic review and
   cross-validation of an endangered species recovery programme}},
Journal = {{JOURNAL FOR NATURE CONSERVATION}},
