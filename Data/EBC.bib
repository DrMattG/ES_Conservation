@article{ ISI:000356813700012,
Author = {Rezac, Milan and Kurka, Antonin and Ruzicka, Vlastimil and Heneberg,
   Petr},
Title = {{Red List of Czech spiders: 3rd edition, adjusted according to
   evidence-based national conservation priorities}},
Journal = {{BIOLOGIA}},
