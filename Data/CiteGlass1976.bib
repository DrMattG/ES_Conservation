@article{ ISI:000258283300012,
Author = {He, Jun and King, William R.},
Title = {{The Role of User Participation in Information Systems Development:
   Implications from a Meta-Analysis}},
Journal = {{JOURNAL OF MANAGEMENT INFORMATION SYSTEMS}},
