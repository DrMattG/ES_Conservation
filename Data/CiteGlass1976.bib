@article{ ISI:000263504600004,
Author = {Nelson, Jon P. and Kennedy, Peter E.},
Title = {{The Use (and Abuse) of Meta-Analysis in Environmental and Natural
   Resource Economics: An Assessment}},
Journal = {{ENVIRONMENTAL \& RESOURCE ECONOMICS}},
Year = {{2009}},
