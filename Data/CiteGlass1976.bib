@article{ ISI:A1995QG57200004,
Author = {SLAVIN, RE},
Title = {{BEST EVIDENCE SYNTHESIS - AN INTELLIGENT ALTERNATIVE TO METAANALYSIS}},
Journal = {{JOURNAL OF CLINICAL EPIDEMIOLOGY}},
Year = {{1995}},
Volume = {{48}},
Number = {{1}},
