@article{ ISI:000182568500010,
Author = {Dalhuisen, JM and Florax, RJGM and de Groot, HLF and Nijkamp, P},
Title = {{Price and income elasticities of residential water demand: A
   meta-analysis}},
Journal = {{LAND ECONOMICS}},
Year = {{2003}},
Volume = {{79}},
Number = {{2}},
Pages = {{292-308}},
Month = {{MAY}},
Abstract = {{This article presents a meta-analysis of variations in price and income
   elasticities of residential water demand. Meta-analysis constitutes an
   adequate tool to synthesize research results by means of an analysis of
   the variation in empirical estimates reported in the literature. We link
   the variation in estimated elasticities to differences in theoretical
   microeconomic choice approaches, differences in spatial and temporal
   dynamics, as well as differences in research design of the underlying
   studies. The occurrence of increasing or decreasing block rate systems
   turns out to be important. With respect to price elasticities, the use
   of the discrete-continuous choice approach is relevant in explaining
   observed differences.}},
