Address = {{750 FIRST ST NE, WASHINGTON, DC 20002-4242 USA}},
Type = {{Review}},
Language = {{English}},
Affiliation = {{Sun, SY (Corresponding Author), Univ Cincinnati, Educ Studies Program, Sch Educ, Dyer Hall 409A,POB 210049, Cincinnati, OH 45221 USA.
   Sun, Shuyan, Univ Cincinnati, Educ Studies Program, Sch Educ, Cincinnati, OH 45221 USA.}},
DOI = {{10.1037/a0019507}},
ISSN = {{0022-0663}},
Keywords = {{NHST; effect size; statistical; significance; practical significance;
