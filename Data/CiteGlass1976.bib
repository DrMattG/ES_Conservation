@article{ ISI:000263577400005,
Author = {Salas, Eduardo and DiazGranados, Deborah and Klein, Cameron and Burke,
   C. Shawn and Stagl, Kevin C. and Goodwin, Gerald F. and Halpin, Stanley
   M.},
Title = {{Does Team Training Improve Team Performance? A Meta-Analysis}},
