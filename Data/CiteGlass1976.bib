@article{ ISI:000085962000004,
Author = {Yin, P and Fan, XT},
Title = {{Assessing the reliability of Beck Depression Inventory scores:
   Reliability generalization across studies}},
Journal = {{EDUCATIONAL AND PSYCHOLOGICAL MEASUREMENT}},
