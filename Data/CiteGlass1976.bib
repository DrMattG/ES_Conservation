@article{ ISI:000251815900003,
Author = {Aguinis, Herman and Sturman, Michael C. and Pierce, Charles A.},
Title = {{Comparison of three meta-analytic procedures for estimating moderating
   effects of categorical variables}},
Journal = {{ORGANIZATIONAL RESEARCH METHODS}},
