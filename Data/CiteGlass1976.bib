@article{ ISI:A1993KQ40800002,
Author = {TRACEY, TJ and ROUNDS, J},
Title = {{EVALUATING HOLLAND AND GATI VOCATIONAL-INTEREST MODELS - A STRUCTURAL
   METAANALYSIS}},
