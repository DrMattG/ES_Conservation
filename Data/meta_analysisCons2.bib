@article{ ISI:000437281500030,
Author = {Cox, Kieran and Brennan, Lawrence P. and Gerwing, Travis G. and Dudas,
   Sarah E. and Juanes, Francis},
Title = {{Sound the alarm: A meta-analysis on the effect of aquatic noise on fish
   behavior and physiology}},
Journal = {{GLOBAL CHANGE BIOLOGY}},
