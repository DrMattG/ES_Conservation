@article{ ISI:000297199000004,
Author = {Amori, Giovanni and Luiselli, Luca},
Title = {{Small mammal community structure in West Africa: a meta-analysis using
   null models}},
Journal = {{AFRICAN JOURNAL OF ECOLOGY}},
